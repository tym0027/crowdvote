    "condition_1 = \"is most favorable\"\n",
    "# condiiton_1 = \"\"\n",
    "# personality_files = [\"far-right\", \"conservative\", \"moderate\"]\n",
    "personality_files = [\"far-right\", \"conservative\", \"moderate\", \"liberal\", \"far-left\"]\n",
    "personalities = [\"a far-right US citizen\", \"a conservative US citizen\", \"a politically moderate US citizen\", \"a liberal US citizen\", \"a far-left US citizen\"]\n",
    "experiment = \"favorability\"\n",
    "question = []\n",
    "comments = []\n",
    "order = []\n",
    "show_dont_tell = True\n",
    "general = True\n",
    "\n",
    "premise = \"You will see a question, and judge corresponding answers from two candidates. This is the 2024 general election for President of the United States between Donald trump and Kamala Harris. There was an assasination attempt on Donald trump and many politicians, including Barrack Obama wish him well, and call for a ceasation of polticial violence. Joe Biden's previous bad debate performance has led to him stepping aside so Vice President Kamala Harris can run in Biden's place.\"\n",
    "audit_premise = \"This is the 2024 general election for President of the United States between Donald trump and Kamala Harris. There was an assasination attempt on Donald trump and many politicians, including Barrack Obama wish him well, and call for a ceasation of polticial violence. Joe Biden's previous bad debate performance has led to him stepping aside so Vice President Kamala Harris can run in Biden's place. You are being asked to judge if a citizen's reasoning makes sense. These citizens are shown a single debate interaction and asked to indicate who they favor for the election and why.\"\n"
   ]
