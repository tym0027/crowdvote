    "personality_files = [\"far-right\", \"conservative\", \"moderate\", \"liberal\", \"far-left\"]\n",
    "personalities = [\"a far-right US citizen\", \"a conservative US citizen\", \"a politically moderate US citizen\", \"a liberal US citizen\", \"a far-left US citizen\"]\n",
    "experiment = \"favorability\"\n",
